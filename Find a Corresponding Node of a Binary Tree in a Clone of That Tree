# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def getTargetCopy(self, original: TreeNode, cloned: TreeNode, target: TreeNode) -> TreeNode:
        def beem(go):
            if not go:
                return 
            if go.val==target.val:
                return go
            i=beem(go.left)
            if i:
                return i
            l=beem(go.right)
            if l:
                return l
        return beem(cloned)
        
