# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def isPalindrome(self, head: Optional[ListNode]) -> bool:
        dabba=[]
        thala=head
        while thala:
            dabba.append(thala.val)
            thala=thala.next
        return True if dabba==dabba[::-1] else False 
        
