class Solution:
    def findClosest(self, x: int, y: int, z: int) -> int:
        if abs(x-z)<abs(y-z):
            return 1
        elif abs(x-z)>abs(y-z):
            return 2
        return 0


#innnonu


class Solution:
    def findClosest(self, x: int, y: int, z: int) -> int:
        if abs(z-x) == abs(y-z):
            return 0
        return 1 if abs(z-x) < abs(y-z) else 2
