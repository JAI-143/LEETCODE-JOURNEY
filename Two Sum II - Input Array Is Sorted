class Solution:
    def twoSum(self, numbers: List[int], target: int) -> List[int]:
        l=len(numbers)
        s,e=0,l-1
        while s<e:
            kp=numbers[s]+numbers[e]
            if kp==target:
                return [s+1,e+1]
            elif kp>target:
                e-=1

            else:
                s+=1
        
