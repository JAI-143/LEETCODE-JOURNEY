class Solution:
    def getRow(self, rowIndex: int) -> List[int]:
        nm=rowIndex+1
        if nm ==1:
            return [1]
        if nm==2:
            return [1,1]
        dabba=[[1],[1,1]]
        for j in range(nm-2):
            potti=[1]
            for k in range(len(dabba[-1])-1):
                potti.append(dabba[-1][k]+dabba[-1][k+1])
            potti+=[1]
            dabba.append(potti)
        return dabba[rowIndex]

            
