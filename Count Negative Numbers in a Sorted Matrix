class Solution:
    def countNegatives(self, grid: List[List[int]]) -> int:
        ennikkai=0
        l=len(grid)
        op=len(grid[0])
        for j in range(l):
            for k in range(-1,-(op+1),-1):
                if grid[j][k]<0:
                    ennikkai+=1
                else:
                    break
        return ennikkai


        
