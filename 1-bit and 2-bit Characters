class Solution:
    def isOneBitCharacter(self, bits: List[int]) -> bool:
        if len(bits)>=2:
                
            b=bits[::-1]
            k=[0]
            l=1
            while b[l]!=0:
                k.append(1)
                l+=1
                if l>=len(b):
                    break
            lop=k[::-1]

        else:
            if bits[0]==0:
                return True
            return False

        
